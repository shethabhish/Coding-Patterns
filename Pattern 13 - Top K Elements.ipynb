    "#Maximum Distinct Elements\n",
    "def distinct(nums,k):\n",
    "    distinctelements = 0\n",
    "    if len(nums) <= k:\n",
    "        return distinctelements\n",
    "    num_freq = {}\n",
    "    for i in nums:\n",
    "        if i not in num_freq:\n",
    "            num_freq[i] = 0\n",
    "        num_freq[i] += 1\n",
    "    \n",
    "    minheap = []\n",
    "    for num,freq in num_freq.items():\n",
    "        if freq == 1:\n",
    "            distinctelements += 1\n",
    "        else:\n",
    "            heappush(minheap,(freq,num))\n",
    "            \n",
    "    while k>0 and minheap:\n",
    "        freq,num = heappop(minheap)\n",
    "        k -= freq - 1\n",
    "        if k >= 0:\n",
    "            distinctelements += 1\n",
    "            \n",
    "    if k > 0:\n",
    "        distinctelements -= k\n",
    "    return distinctelements"
   ]
