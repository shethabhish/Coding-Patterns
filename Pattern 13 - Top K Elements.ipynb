    "#Frequency Sort \n",
    "def frequency(s):\n",
    "    charfreq = {}\n",
    "    for i in s:\n",
    "        if i not in charfreq:\n",
    "            charfreq[i] = 0\n",
    "        charfreq[i] += 1\n",
    "    maxheap = []\n",
    "    for char,freq in charfreq.items():\n",
    "        heappush(maxheap,(-freq,char))\n",
    "    ans = []\n",
    "    while maxheap:\n",
    "        freq,char = heappop(maxheap)\n",
    "        for _ in range(-freq):\n",
    "            ans.append(char)\n",
    "    return ''.join(ans)"
   ]
