    "#Kth Smallest Number\n",
    "from heapq import *\n",
    "def ksmallest(nums,k):\n",
    "    maxheap = []\n",
    "    for i in range(k):\n",
    "        heappush(maxheap,-nums[i])\n",
    "    for i in range(k,len(nums)):\n",
    "        if -nums[i] > maxheap[0]:\n",
    "            heappop(maxheap)\n",
    "            heappush(maxheap,-nums[i])\n",
    "    return -maxheap[0]"
   ]
