    "#Kth Largest Number in a Stream\n",
    "class kthlargest:\n",
    "    minheap = []\n",
    "    def __init__(self,nums,k):\n",
    "        self.k = k\n",
    "        for num in nums:\n",
    "            self.add(num)\n",
    "    def add(self,num):\n",
    "        heappush(self.minheap,num)\n",
    "        if len(self.minheap) > self.k:\n",
    "            heappop(self.minheap)\n",
    "        return self.minheap[0]"
   ]
