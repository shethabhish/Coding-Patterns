    "#Top 'K' Numbers\n",
    "from heapq import *\n",
    "def topk(nums,k):\n",
    "    minheap = []\n",
    "    for i in range(k):\n",
    "        heappush(minheap,nums[i])\n",
    "    for i in range(k,len(nums)):\n",
    "        if nums[i] > minheap[0]:\n",
    "            heappop(minheap)\n",
    "            heappush(minheap,nums[i])\n",
    "    return minheap"
   ]
